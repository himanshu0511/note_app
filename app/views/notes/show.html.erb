<%= render :partial => 'note' %>
<% if @note.created_by_id == current_user.id or NoteSharing.where(:note_id => @note.id, :user_id => current_user.id).count() > 0 %>
    <%= link_to 'Update', edit_note_path(@note) %>
<% end %>
<% if @note.created_by_id == current_user.id %>
    <%= link_to 'Delete', @note, method: :delete, data: {confirm: 'Are you sure?'} %>
<% end %>